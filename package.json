{
  "name": "dev-ops",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "files": [
    "dist/apps/client",
    "dist/apps/api"
  ],
  "scripts": {
    "dev": "npm run concur -- \"npm run start:fe\" \"npm run start:be\"",
    "start:fe": "ng serve client",
    "start:be": "ng serve api",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "concur": "concurrently -p=\"{name}\" -n=\"Angular,NestJS\" -c=\"magenta,blue\"",
    "build": "npm run concur -- \"npm run build:fe\" \"npm run build:be\"",
    "build:prod": "npm run concur -- \"npm run build:fe:prod\" \"npm run build:be:prod\"",
    "build:fe": "nx build client",
    "build:be": "nx build api",
    "build:fe:prod": "npm run build:fe -- --prod",
    "build:be:prod": "npm run build:be -- --prod",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prettify": "prettier \"apps/**/*.ts\"",
    "prettify:fix": "npm run prettify -- --write"
  },
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nrwl/angular": "12.0.8",
    "helmet": "^4.6.0",
    "ng-sidebar": "^9.4.2",
    "ngx-smart-modal": "^7.4.1",
    "ngx-toastr": "^13.2.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "quotesy": "^1.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "~2.0.2",
    "@angular-eslint/eslint-plugin-template": "~2.0.2",
    "@angular-eslint/template-parser": "~2.0.2",
    "@angular/cli": "~11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "12.0.8",
    "@nrwl/cypress": "12.0.8",
    "@nrwl/eslint-plugin-nx": "12.0.8",
    "@nrwl/jest": "12.0.8",
    "@nrwl/linter": "12.0.8",
    "@nrwl/nest": "12.0.8",
    "@nrwl/node": "12.0.8",
    "@nrwl/tao": "12.0.8",
    "@nrwl/workspace": "12.0.8",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "concurrently": "^6.0.2",
    "cypress": "^6.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^4.3.8",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "prettier": "2.2.1",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:fix"
    }
  }
}
